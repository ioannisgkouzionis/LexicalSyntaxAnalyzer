(*  print multiples of PI *)
program print_pi;

@defmacro PI 3.1415927
@defmacro PI_2 (PI/2)
@defmacro PI2  (PI*2)

begin
	// writeString('PI='); writeReal(PI); writeString('\n');
	writeString('PI/2='); writeReal(PI_2); writeString('\n');
	writeString('2*PI='); writeReal(PI2); writeString('\n')
	i=0;
	i=00015;
	i=.5;
	i=5.5;
end.